{
  "name": "space-portfolioapp",
  "version": "1.0.0",
  "description": "Cool personal app that consumes Nasa's and SpaceX APIs to see some cool content such as rocket missions and Nasa's awesome repository of info/images. Stack: React, Apollo, Express, GraphQl",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && node ./build/server.js",
    "server": "nodemon --exec babel-node server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./ -s",
    "build": "npm run clean && npm run build-babel"
  },
  "keywords": [
    "SpaceX",
    "Nasa",
    "GraphQl",
    "React",
    "Apollo",
    "Client"
  ],
  "author": "Jose Luis Raffalli Adriani",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-cli": "^6.26.0",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphql": "^14.0.2",
    "react-apollo": "^2.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.0",
    "nodemon": "^1.18.9"
  }
}
